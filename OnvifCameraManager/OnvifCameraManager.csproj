    <PackageReference Include="SharpOnvifClient" Version="0.4.1" />
    <PackageReference Include="SharpOnvifClient.DeviceMgmt" Version="0.4.1" />
    <PackageReference Include="SharpOnvifClient.Media" Version="0.4.1" />
  </ItemGroup>

</Project>
